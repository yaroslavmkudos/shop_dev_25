    "c4c27882-8032-4cb7-9514-d77f0ab5e11f": {
      "type": "landing-what-is-a",
      "settings": {
        "title": "What are Twitch Panels?",
        "image": "shopify://shop_images/what_are_twitch_panels.png",
        "text": "<div>Twitch panels are images, text blocks or interactive extensions that you can place in the About section of your Twitch profile.<br><br>They provide your viewers with useful information about your stream and effectively convey your branding. Commonly used panels include images with links to your social media profiles, donation and subscription pages.<br><br>On top of that, Twitch allows you to use special markdown to stylize plain text under your panels with emphasis, lists, quotes and headings.</div>",
        "background": "#282a2e",
        "color": "#ffffff"
      }
    },
    "e379e172-4a2b-4474-811a-a677fe2303bb": {
      "type": "landing-tutorial",
      "settings": {
        "title": "How to Add Panels on Twitch?",
        "text": "<div>\n<ol>\n<li>Log in to your Twitch account\n</li>\n<li>Click on your profile picture at the top-right side of the screen and go to \"Channel”\n</li>\n<li>Click the \"About\" button under the channel name\n</li>\n<li>Click on \"Edit Panels\" at the bottom of the screen\n</li>\n<li>Press the + window and choose \"Add a Text or Image Panel”</li>\n<li>Press \"Add Image\", choose the panel you want to upload, and press \"Done”</li>\n<li>Then add a description text and URL (if needed) to the other fields</li>\n<li>That's it! Your panel is up and running :)</li>\n</ol>\n</div>",
        "background_color": "#f9f9f9"
      }
    },
    "c15c78c1-e79f-4af4-bd81-b3e7166a6e2f": {
      "type": "landing-guide",
      "blocks": {
        "27f7e4d4-1a3d-4061-a5d6-9d039c366454": {
          "type": "guide_item",
          "settings": {
            "title": "Text Emphasis",
            "table": "<div class=\"row\">\n<div>Bold Text</div>\n<div>**Bold**</div>\n</div>\n\n<div class=\"row\">\n<div>Italic</div>\n<div>*Italic*</div>\n</div>\n\n<div class=\"row\">\n<div>Link</div>\n<div>[Link](https://kudos.tv)</div>\n</div>\n\n<div class=\"row\">\n<div>Header 1</div>\n<div>#Your Text</div>\n</div>\n\n<div class=\"row\">\n<div>Header 2</div>\n<div>##Your Text</div>\n</div>\n\n<div class=\"row\">\n<div>Header 3</div>\n<div>###Your Text</div>\n</div>",
            "image": "shopify://shop_images/image_43.jpg"
          }
        },
        "65d3f1ed-9ab5-45fa-9d80-966059cd2965": {
          "type": "guide_item",
          "settings": {
            "title": "Text Formatting",
            "table": "<div>\n<div class=\"row\">\n<div>Quote</div>\n<div>\n&gt;This is a quote!\n</div>\n</div>\n\n<div class=\"row\">\n<div>Nested Quote</div>\n<div>\n&gt;&gt;This is a nested quote!\n</div>\n</div>\n\n</div>",
            "image": "shopify://shop_images/twitch_formatting.png"
          }
        },
        "3b983c41-7acf-4547-8550-4e7efa8228d5": {
          "type": "guide_item",
          "settings": {
            "title": "Lists",
            "table": "<div class=\"row\">\n<div>Unordered list</div>\n<div>\n* Item 1\n<br>\n* Item 2\n<br>\n* Item 3\n</div>\n</div>\n\n<div class=\"row\">\n<div>Ordered list</div>\n<div>\n1. Item 1\n<br>\n2. Item 2\n<br>\n3. Item 3\n<br>\n</div>\n</div>",
            "image": "shopify://shop_images/image_45.jpg"
          }
        }
      },
